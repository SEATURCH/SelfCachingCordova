{
    "root": [
        {
            "property": "model_1_name",
            "type": "model_1"
        },
        {
            "property": "model_2_name",
            "type": "model_2"
        },
        {
            "property": "model_3_name",
            "type": "model_3"
        }
    ],
    "model_3": [
        {
            "property": "ComplexArray",
            "title": "Complex Array",
            "type": "Array",
            "nestedType": "model_1"
        },
        {
            "property": "SimpleArray",
            "type": "Array",
            "nestedType": "string"
        }
    ],
    "model_1": [
        {
            "property": "ProertName",
            "title": "First",
            "type": "string"
        }
    ],
    "model_2": [
        {
            "property": "PropertyName",
            "title": "Property Name",
            "type": "string",
            "validation": [
                {
                    "conditions": [
                        {
                            "target": "SiblingProperty_1",
                            "type": "hasValue"
                        }
                    ],
                    "rules": [
                        {
                            "type": "hasValue",
                            "error": "Property Name cannot be null if Sibling Property is set"
                        }
                    ]
                },
                {
                    "rules": [
                        {
                            "type": "isPattern",
                            "args": "/[^0-9]/",
                            "error": "This value cannot contain a number"
                        }
                    ]
                }
            ]
        },
        {
            "property": "SiblingProperty_1",
            "title": "Sibling Propert 1",
            "type": "options",
            "options": [ "Value 1", "Value 2", "Value 3" ],
            "validation": [
                {
                    "rules": [
                        {
                            "type": "hasValue",
                            "error": "Sibling Propert 1 cannot be null"
                        }
                    ]
                }
            ]
        },
        {
            "property": "SiblingProperty_2",
            "title": "Sibling Propert 2",
            "type": "number"
        },
        {
            "property": "DateProp",
            "title": "Date Property",
            "type": "date"
        }
    ]
}